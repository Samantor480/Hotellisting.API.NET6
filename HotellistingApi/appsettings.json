{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }

    },
    "WriteTo": [

      {

        "Name": "File",
        "Args": {
          "Path": "/logs/log-.txt",
          "rollinginterval": "Day"
        }
      },
      {
        "Name": "seq",
        "Args": {"serverurl": "http://localhost5341"}
      }
    ]
  },
  "AllowedHosts": "*"
}
